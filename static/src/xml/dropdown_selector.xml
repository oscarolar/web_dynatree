<?xml version="1.0" encoding="UTF-8"?>
<!-- vim:fdl=1:
-->
<templates id="template" xml:space="preserve">
  <t t-name="SearchDynatreeSelectionField">
    <label t-att-title="attrs.help"
           t-att-class="'oe_label' + (attrs.help ? '_help' : '')"
           for="dynatree"
           t-att-style="style">
        <t t-esc="attrs.string || attrs.name"/>
        <span t-if="attrs.help">?</span>
    </label>
    <div>
    <div id="dynatree" class="oe_dynatree"></div>
    <input type="text" size="15" id="dynatree-summary" class="openerp" />
    </div>
  </t>
</templates>
